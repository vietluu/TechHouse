{
  "name": "techhouse",
  "version": "1.1.0",
  "private": true,
  "license": "MIT",
  "author": "vietluu",
  "engines": {
    "node": ">=16.8.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx": [
      "eslint --fix",
      "git add --force"
    ],
    "*.{json,md,tsx,ts,css}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "SET ENVIRONMENT=./.env &&  node server.js",
    "format": "prettier --write .",
    "prepare": "husky install",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@types/react-slick": "^0.23.10",
    "@types/slick-carousel": "^1.6.37",
    "antd": "^5.6.2",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "cookies-next": "^2.1.2",
    "critters": "^0.0.19",
    "eslint": "^8.43.0",
    "eslint-config-next": "13.4.6",
    "express": "^4.18.2",
    "img-optimize-loader": "^1.0.7",
    "js-cookie": "^3.0.5",
    "next": "^13.4.16",
    "nextjs-toploader": "^1.4.2",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-facebook": "^9.0.12",
    "react-redux": "^8.1.1",
    "react-slick": "^0.29.0",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "sass": "^1.64.1",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.6",
    "@types/express": "^4.17.17",
    "clean-css": "^5.3.2",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "terser-webpack-plugin": "^5.3.9",
    "thread-loader": "^4.0.2"
  }
}
